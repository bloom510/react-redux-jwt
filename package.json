{
  "name": "comments",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "express": "^4.16.3",
    "jwt-simple": "^0.5.1",
    "mongoose": "^5.2.9",
    "morgan": "^1.9.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0",
    "tv4": "^1.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.0"
  },
  "scripts": {
    "setup": "yarn && cd client && yarn",
    "deploy": "yarn build && yarn checkout",
    "checkout": "git checkout -b deploy && npm run add",
    "add": "git add -f client/build && npm run commit",
    "commit": "git commit -m \"deploy\" && npm run push",
    "push": "git push -f heroku deploy:master && npm run master",
    "master": "git checkout master && npm run delete",
    "delete": "git branch -D deploy",
    "start": "react-scripts start",
    "build": "node internals/scripts/build.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "client": "cd client && yarn start",
    "server": "nodemon server",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  }
}
